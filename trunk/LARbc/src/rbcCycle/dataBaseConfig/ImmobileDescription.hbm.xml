<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">
	<class name="rbcCycle.caseElement.ImmobileDescription" table="casos">
		<id name="idCase" column="id_Caso">
			<generator class="native" />
		</id>

		<property name="state" column="estado" />
		<property name="city" column="cidade" />
		<property name="neighborhood" column="bairro" />
		<property name="street" column="rua" />
		<property name="name" column="nome" />
		<property name="builtArea" column="areaConstruida" />
		<property name="totalArea" column="areaTotal" />
		<property name="garageSpace" column="vagasGaragem" />
		<property name="bedroom" column="quartos" />
		<property name="suite" column="suites" />
		<property name="bathroom" column="banheiros" />
		<property name="type" column="tipo" />
		<property name="price" column="preco" />
		<property name="businessType" column="tipoNegocio" />
	</class>
</hibernate-mapping>
